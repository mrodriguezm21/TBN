{
  "name": "tbn",
  "version": "1.0.0",
  "description": "Prueba tecnica The Biz Nation",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon index.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "seeders:generate": "sequelize-cli seed:generate --name",
    "seeders:run": "sequelize-cli db:seed:all",
    "seeders:revert": "sequelize-cli db:seed:undo",
    "seeders:delete": "sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrodriguezm21/TBN.git"
  },
